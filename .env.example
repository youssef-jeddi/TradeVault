# Client (Vite) env variables
# Copy to .env and fill in values

# Required: Privy app id for auth
VITE_PRIVY_APP_ID=

# Optional: auto-load on-chain Protected Data listings (true/false)
VITE_AUTO_LOAD_PD=false

# Optional: iExec explorer network slug (e.g., bellecour, viviani)
VITE_IEXEC_EXPLORER_SLUG=bellecour
